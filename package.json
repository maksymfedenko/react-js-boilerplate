{
  "name": "Anonym",
  "license": "UNLICENSED",
  "private": true,
  "version": "2.0.0",
  "main": "index.js",
  "author": "anonym",
  "scripts": {
    "build:only": "webpack --config ./config/webpack.production.js --mode production",
    "build": "npm run clean && npm run build:only && npm run copyAssets",
    "start": "webpack-dev-server --config ./config/webpack.development.js --mode development --open",
    "clean": "rimraf build",
    "clean:all": "npm run clean && npm run markup:clean",
    "copyAssets": "cp -R ./src/assets/* ./build",
    "serve": "static build",
    "lint": "bash -c 'eslint --ext=js --ext=jsx ${1-./src}' -- ",
    "lint:fix": "bash -c 'eslint --ext=js --ext=jsx --fix ${1-./src}' -- ",
    "lint:css": "bash -c 'stylelint \"${1-./src}/**/*.scss\"' -- ",
    "lint:css:fix": "bash -c 'stylelint --fix \"${1-./src}/**/*.scss\"' -- ",
    "add": "docker exec ddt_react npm i",
    "remove": "docker exec ddt_react npm uninstall"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^1.0.0-rc.6",
    "lint-staged": "^7.1.0",
    "mini-css-extract-plugin": "^0.3.0",
    "node-sass": "^4.8.3",
    "node-static": "^0.7.10",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "prettier": "^1.12.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "glslify": "^6.1.0",
    "immutable": "^3.8.2",
    "js-util": "git://github.com/ykob/js-util.git",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "three": "0.89.0"
  }
}
